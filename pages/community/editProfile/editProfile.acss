page {
  background-color: rgb(240, 240, 240)
}

.weui-cell {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: rgb(255, 255, 255);
  height: 0.9rem;
  font-size: 0.4rem;
  border-bottom: solid rgb(240, 240, 240) 2rpx;
}

.weui-cell__ft {
  flex: 1;
  text-align: right;
  color: #808080;
  margin-right: 20rpx;
}

.weui-cell__bd {
  padding-left: 30rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 0.5 1 0%;
}

.thumb {
  width: 0.8rem;
  height: 0.8rem;
  border-radius: 1rem;
}

.arrow {
  display: block;
  width: 0.3rem;
  height: 0.3rem;
  margin-right: 0.2rem;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50% 50%;
}

.bg_view {
  background-color: rgb(240, 240, 240);
}

.tapBtn {
  border-radius: 20px;
  background-color: rgb(50, 171, 240);
  border: 0px solid;
  height: 0.7rem;
  position: fixed;
  width: 86%;
  margin-left: 7%;
  bottom: 2rem;
  line-height: 0.7rem;
  box-shadow: rgba(153, 153, 153, 0.24) 0 0.01rem 0.2rem;
  font-size: 0.35rem;
  color: #fff;
}